<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fcMainActivity"
        android:name="com.example.coffeetruckfinalproject11.Module.Truck.ListViewTrucks"
        android:layout_width="406dp"
        android:layout_height="732dp"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/NavBarFragment"
        android:name="com.example.coffeetruckfinalproject11.NavBar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="667dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="64dp"
        android:background="#009688"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.87" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fCAddNewCoffeeTruck"
        android:name="com.example.coffeetruckfinalproject11.AddNewCoffeeTruck"
        android:layout_width="112dp"
        android:layout_height="93dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="547dp" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fcUserProfileCeation"
        android:name="com.example.coffeetruckfinalproject11.UserProfileCreation"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/FrameLayoutProfile"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="9dp"
        app:layout_constraintStart_toEndOf="@+id/fcMainActivity"
        app:layout_constraintTop_toBottomOf="@+id/fcMainActivity">

    </FrameLayout>

    <Button
        android:id="@+id/btnCreateNewProfile"
        android:layout_width="379dp"
        android:layout_height="78dp"
        android:text="CREATE NEW PROFILE"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="329dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
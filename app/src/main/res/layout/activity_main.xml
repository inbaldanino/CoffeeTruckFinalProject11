<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fcMainActivity"
        android:name="com.example.coffeetruckfinalproject11.ListViewTrucks"
        android:layout_width="406dp"
        android:layout_height="732dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="-1dp" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fCAddNewCoffeeTruck"
        android:name="com.example.coffeetruckfinalproject11.AddNewCoffeeTruck"
        android:layout_width="349dp"
        android:layout_height="447dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="193dp" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fcUserProfileCeation"
        android:name="com.example.coffeetruckfinalproject11.UserProfileCreation"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/FrameLayoutProfile"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="9dp"
        app:layout_constraintStart_toEndOf="@+id/fcMainActivity"
        app:layout_constraintTop_toBottomOf="@+id/fcMainActivity">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>